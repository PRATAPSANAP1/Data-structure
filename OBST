#include<iostream>
using namespace std;
class obst
{
public:
int i,j;
void weight(int i,int j,int sp[],int usp[])
{
if(i=j)
{
cout<<usp[i];
}
else
{
cout<<weight[i,j-1]+ps[j]+usp[j];
}
}


};
int main()
{
obst o;
int n,i,j;
cout<<"Enter the Number of keys:";
cin>>n;
int sp[n],usp[n+1],keys[n];
cout<<"Enter the Keys:";
for(i=0;i<n;i++)
{
cin>>keys[i];
}
cout<<"Enter the successful probability:";
for(i=0;i<n;i++)
{
cin>>sp[i];
}
cout<<"Enter the unsuccessful probability:";
for(i=0;i<n+1;i++)
{
cin>>usp[i];
}
for(i=0;i<n;i++)
{
for(j=0;j<n;j++)
{
if(j-i<n)
{
o.weight(i,j,sp[],usp[]);
}
else
{

}
}
}
